{
  "name": "n8n-nodes-zoho-expense-by-datingscout",
  "version": "1.2.2",
  "description": "n8n community node for Zoho Expense API - Full implementation with all features",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "zoho",
    "zoho expense",
    "expense management",
    "expense tracking"
  ],
  "license": "MIT",
  "homepage": "https://github.com/datingscout/n8n-nodes-zoho-expense-by-datingscout",
  "author": {
    "name": "DatingScout",
    "email": "dev@datingscout.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datingscout/n8n-nodes-zoho-expense-by-datingscout.git"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ZohoExpenseOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ZohoExpense/ZohoExpense.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
